{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/login": {
      "target": "http://localhost:5000",
      "secure": false
    },
    "/logout": {
      "target": "http://localhost:5000",
      "secure": false
    },
    "/graphql": {
      "target": "http://localhost:5000",
      "secure": false
    }
  },
  "scripts": {
    "start": "npm run sass:build && react-scripts start",
    "build": "react-scripts build",
    "sass:watch": "node-sass -w src/styles/scss -o src/styles/css",
    "sass:build": "node-sass src/styles/scss -o src/styles/css --source-map true",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "apollo-boost": "^0.1.10",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "bulma": "^0.7.1",
    "dotenv": "^6.0.0",
    "enzyme-to-json": "^3.3.4",
    "moment": "^2.22.2",
    "react": "^16.4.0",
    "react-apollo": "^2.1.6",
    "react-dates": "^16.7.0",
    "react-dom": "^16.4.0",
    "react-places-autocomplete": "^7.1.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "sinon": "^6.0.1",
    "spotify-web-api-node": "^3.1.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "node-sass": "^4.9.0"
  }
}
